pipeline {
    agent any

   // tools {
     //   maven 'Maven_3.6.3' // Ensure this matches your Maven configuration name
    //}

    stages {
        stage('Checkout Code') {
            steps {
                // Check out the code from the GitHub repository
                checkout scm
            }
        }

        stage('Print Current Branch') {
            steps {
                script {
                    // Use a shell command to fetch the current branch name
                    def branchName = sh(
                        script: "git rev-parse --abbrev-ref HEAD",
                        returnStdout: true
                    ).trim()
                    echo "Current Branch Name: ${branchName}"
                }
            }
        }
    }
}
