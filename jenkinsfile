pipeline {
    agent any

   // tools {
     //   maven 'Maven_3.6.3' // Ensure this matches your Maven configuration name
    //}

    stages {
        stage('Checkout Code') {
            steps {
                // Checkout code from Git
                checkout([$class: 'GitSCM',
                          branches: [[name: '*/main']],
                          userRemoteConfigs: [[url: 'https://github.com/karthikvijay139/java-project.git']]
                ])
            }
        }
        stage('Print Branch Name') {
            steps {
                // Print the current branch name
                script {
                    def branchName = sh(
                        script: 'git rev-parse --abbrev-ref HEAD',
                        returnStdout: true
                    ).trim()
                    echo "Current branch: ${branchName}"
                }
            }
        }
      //  stage('Build with Maven') {
        //    steps {
                // Build the project with Maven
          //      sh 'mvn clean install'
            //}
        //}
    //}
}


